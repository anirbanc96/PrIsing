MSE.plot.f <- function(N, J.N, beta1, beta2, epsilon.vec, delta,
priv = T, n.rep){
MSE.p.le1 <- rep(0, length(epsilon.vec))
MSE.flip.le1 <- rep(0, length(epsilon.vec))
MSE.p.ge1 <- rep(0, length(epsilon.vec))
MSE.flip.ge1 <- rep(0, length(epsilon.vec))
for (i in 1:length(epsilon.vec)){
beta.est.iter.priv <- sim.beta.est(n.rep, beta1, N, J.N,
priv = T, epsilon.vec[i], delta)
MSE.p.le1[i] <- sum((beta.est.iter.priv[,1] - beta1)^2)/n.rep
MSE.flip.le1[i] <- sum((beta.est.iter.priv[,2] - beta1)^2)/n.rep
beta.est.iter.priv <- sim.beta.est(n.rep, beta2, N, J.N,
priv = T, epsilon.vec[i], delta)
MSE.p.ge1[i] <- sum((beta.est.iter.priv[,1] - beta2)^2)/n.rep
MSE.flip.ge1[i] <- sum((beta.est.iter.priv[,2] - beta2)^2)/n.rep
print (i)
}
MSE.dat <- cbind(epsilon, MSE.p.le1, MSE.p.ge1, MSE.flip.le1, MSE.flip.ge1)
colnames(MSE.dat) <- c("Epsilon", "p.le1", "p.ge1", "flip.le1", "flip.ge1")
MSE.dat <- as_tibble(MSE.dat)
MSE.dat[,1] <- as.factor(epsilon.vec)
MSE.plot <- MSE.dat %>%
ggplot(aes(x = Epsilon, y = p.le1)) +
geom_point(aes(x = Epsilon, y = p.le1), col = "navy") +
geom_line(aes(x = Epsilon, y = p.le1, col = "PBL1", group = Epsilon[1])) +
geom_point(aes(x = Epsilon, y = p.ge1), col = "darkorange") +
geom_line(aes(x = Epsilon, y = p.ge1, col = "PBG1", group = Epsilon[2])) +
geom_point(aes(x = Epsilon, y = flip.le1), col = "darkred") +
geom_line(aes(x = Epsilon, y = flip.le1, col = "FBL1", group = Epsilon[3])) +
geom_point(aes(x = Epsilon, y = flip.ge1), col = "darkmagenta") +
geom_line(aes(x = Epsilon, y = flip.ge1, col = "FBG1", group = Epsilon[4])) +
scale_colour_manual("Methods", breaks = c("PBL1", "PBG1", "FBL1", "FBG1"),
values = c("cornflowerblue", "orange", "red", "plum"),
labels = c(expression(paste(beta, " < ", 1, " Objective Perturbation")),
expression(paste(beta, " > ", 1, " Objective Perturbation")),
expression(paste(beta, " < ", 1, " Randomized Response")),
expression(paste(beta, " > ", 1, " Randomized Response")))) +
labs(title = TeX(r'(MSE of MPLE)'), x = TeX(r'($\epsilon$)'),
y = TeX(r'(MSE)'), color = "Legend Title\n") +
theme_bw() +
theme(plot.title = element_text(hjust = 0.5),
legend.position = c(0.75,0.65),
legend.text.align = 0,
legend.background = element_blank(),
legend.box.background = element_rect(colour = "black"))
MSE.plot
return (MSE.plot)
}
